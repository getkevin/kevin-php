{
    "name": "getkevin/kevin-php",
    "description": "PHP client for kevin. platform.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kevin.",
            "email": "help@kevin.eu"
        }
    ],
    "support": {
        "email": "help@kevin.eu"
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix --config ./.php.cs.fixer.php --allow-risky=yes",
        "check-style": "vendor/bin/php-cs-fixer fix --config ./.php.cs.fixer.php --allow-risky=yes --dry-run --verbose"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19"
    },
    "require": {
        "php": ">=5.6",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Kevin\\": "src/"
        }
    }
}
